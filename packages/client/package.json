{
  "name": "client",
  "version": "0.0.0",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "tsc --project tsconfig.server.json && cross-env NODE_ENV=development EXTERNAL_SERVER_URL='http://localhost:3001' INTERNAL_SERVER_URL='http://localhost:3001' node server/index.js",
    "build": "tsc --project tsconfig.server.json && vite build && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "node server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "test": "jest ./",
    "prepare": "husky",
    "prebuild": "tsc --noEmit",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-helmet": "^6.1.7",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "redux": "^4.2.1",
    "serialize-javascript": "^6.0.1",
    "styled-components": "^6.1.0",
    "vite": "7.1.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --config ../../.eslintrc.cjs --ignore-pattern 'src/slices/**/*' --ignore-pattern 'src/pages/**/*' --ignore-pattern 'src/components/**/*' --ignore-pattern 'src/App.tsx' --ignore-pattern 'src/App.test.tsx'",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-utils": "^3.0.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "lint-staged": "^16.2.3",
    "prettier": "2.8.8",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "typescript-eslint": "^8.0.0"
  },
  "license": "MIT"
}
